#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string s;
    cin>>s;
    string s2="";
    for(int i=0;i<n;i++)
    {
        int one=(int)s[i];
        int two=(int)s[i+1];
        if(abs(one-two)!=32)
        {
            s2=s2+s[i];
        }
        else{
            i++;
        }
    }
    cout<<s2;
}
