#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,k;
    cin>>n>>k;
    unordered_map<int,int>m;
    int res=-1;
    for(int i=0;i<n;i++)
    {
        int arr;
        cin>>arr;
        m[arr]++;
        if(m[arr]>=k)
        {
            res=arr;
            break;
        }
    }
    cout<<res;
   
}
