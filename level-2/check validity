#include <iostream>
#include <stack>
#include <string>
using namespace std;

bool isValid(string s) {
    stack<char> st;
    
    for (char c : s) {
        if (c == '(' || c == '[' || c == '{') {
            st.push(c);
        } else {
            if (st.empty()) {
                return false;
            }
            
            char top = st.top();
            if ((c == ')' && top != '(') ||
                (c == ']' && top != '[') ||
                (c == '}' && top != '{')) {
                return false;
            }
            
            st.pop();
        }
    }
    
    return st.empty();
}

int main() {
    int N;
    cin >> N;
    cin.ignore();  // Consume newline character
    
    while (N--) {
        string input;
        getline(cin, input);
        bool result = isValid(input);
        cout << (result ? "True" : "False") << endl;
    }

    return 0;
}
