#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int fives=0;
    int tens=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]==5)
        {
            fives++;
        }
        else if(arr[i]==10){
            if(fives>=1)
            {
                fives--;
                tens++;
            }
            else{
                cout<<"False"<<endl;
                return 0;
            }

        }
        else if(arr[i]==20)
        {
            if(tens>=1 && fives>=1)
            {
                fives--;
                tens--;
            }
            else if(fives>=3)
            {
                fives=fives-3;
            }
            else{
                cout<<"False"<<endl;
                return 0;
            }
        }
        }
        cout<<"True"<<endl;
        return 0;

    
}
