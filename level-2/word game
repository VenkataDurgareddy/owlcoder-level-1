#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    cin>>s;
    int k;
    cin>>k;
    int lene=s.length();
    if(lene%k==0)
    {
        int m=lene/k;
        for(int i=0;i<lene;i++)
        {
            if(i>0 and i%m==0)
            {
                cout<<" ";
            }
            cout<<s[i];
        }
    }
    else{
        cout<<"Invalid String";
    }
}
