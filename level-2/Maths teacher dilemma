#include<bits/stdc++.h>
using namespace std;
int gcd(int a, int b)
{
    while(a!=b)
    {
        if(a>b)
        {
            a=a-b;
        }
        else{
            b=b-a;
        }
    }
    return a;
}
int main()
{
    int a,b;
    cin>>a>>b;
    int gcd_2=gcd(a,b);
    int lcm=(a*b)/gcd_2;
    cout<<lcm<<" "<<gcd_2;

}
